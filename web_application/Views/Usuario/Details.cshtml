@model Entities.Usuario

@{
    ViewBag.Title = "Details";
}

<h1 class="page-title">
    Mi perfil
    @*<small>mejor conocido como...</small>*@
</h1>
@{  //Para mostrar solo la fecha sin la hora
    String newfecha = "";
    int c = 0;
    String value = (Model.FechaNacimiento).ToString();
    Char delimiter = ' ';
    String[] substrings = value.Split(delimiter);
    foreach (var substring in substrings)
    {
        if (c == 0)
        {
            newfecha = substring;
        }
        c++;

    }

}

